{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "coverage": {
      "dependsOn": ["^build"],
      "outputs": ["coverage"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "release": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}